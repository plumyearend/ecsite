{
    "preset": "laravel",
    "cache-file": ".pint.cache",
    "exclude": [
        "database/migrations"
    ],
    "rules": {
        "not_operator_with_successor_space": false,
        "single_line_empty_body": false,
        "new_with_parentheses": false,
        "concat_space": {
            "spacing": "one"
        },
        "class_attributes_separation": {
            "elements": {
                "const": "one",
                "method": "one",
                "trait_import": "none"
            }
        }
    }
}